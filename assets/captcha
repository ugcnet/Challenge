        $(document).ready(function () {
          $(".userActivityBtn").click(function () {
            console.log("login-main.php");
            var currentURL   = $(location).attr("href");
              $.ajax({
                url: "/frontend/web/useracitivity/useract" ,
                type: "post",
                data: {
                      moduleName: $(this).attr("data-modulename"),  
                      activityName: $(this).attr("data-activityname"), 
                      url: currentURL,  
                      _csrf : "<?=Yii::$app->request->getCsrfToken()?>"
                    },
                success: function (data) {
                   console.log(data);
                }
            });
          })
        });

       
});</script>
