jQuery('#login-form').yiiActiveForm([{"id":"loginform-applicationnumber","name":"ApplicationNumber","container":".field-loginform-applicationnumber","input":"#loginform-applicationnumber","error":".invalid-feedback","validate":function (attribute, value, messages, deferred, $form) {yii.validation.required(value, messages, {"message":"Application Number cannot be blank."});yii.validation.regularExpression(value, messages, {"pattern":/^[0-9]+$/,"not":false,"message":"Application Number should contain only numeric value.","skipOnEmpty":1});yii.validation.string(value, messages, {"message":"Application Number must be a string.","min":12,"tooShort":"Application Number should contain at least 12 characters.","max":30,"tooLong":"Application Number should contain at most 30 characters.","skipOnEmpty":1});}},{"id":"Password","name":"Password","container":".field-Password","input":"#Password","error":".invalid-feedback","validate":function (attribute, value, messages, deferred, $form) {yii.validation.required(value, messages, {"message":"Password cannot be blank."});}},{"id":"my-captcha-image","name":"verifyCode","container":".field-my-captcha-image","input":"#my-captcha-image","error":".invalid-feedback","validate":function (attribute, value, messages, deferred, $form) {yii.validation.required(value, messages, {"message":"Verification Code cannot be blank."});yii.validation.regularExpression(value, messages, {"pattern":/^[0-9]+$/,"not":false,"message":"Verification Code should contain only numeric value.","skipOnEmpty":1});yii.validation.captcha(value, messages, {"hash":6333,"hashKey":"yiiCaptcha\/site\/captcha","caseSensitive":false,"message":"The verification code is incorrect."});}}], {"errorSummary":".alert.alert-danger","errorCssClass":"is-invalid","successCssClass":"is-valid","validationStateOn":"input"});

        $(document).ready(function () {
            $("#refresh_captcha").click(function (event) {
                event.preventDefault();
                $(this).parent().children('img').click();
            })
        });

        $(document).ready(function (){
            $(".form-control").addClass("form-control-sm");
