    <script>
      function numericApplication(e, action = "alpha") {
        var k = e.keyCode;
        var applicationnumber = $('#loginform-applicationnumber').val().length
        //console.log(applicationnumber);
        if (applicationnumber > 30) {
          return false;
        }
        var status = ((k > 64 && k < 91) || (k > 96 && k < 123) || k == 8 || k == 32);
        if (action == "alpha") {
          status = ((k > 64 && k < 91) || (k > 96 && k < 123) || k == 8 || k == 32);
        } else if (action == "numeric") {
          status = ((k >= 48 && k <= 57));
        }
        return status;
      }

      function turnOnPasswordStyle() {
        $('#Password').attr('type', "password");
      }
      document.getElementById("togglePassword").addEventListener("click", eyeOpenHide);

      function eyeOpenHide() {
        // var password = document.querySelector("#Password");
        var password = document.getElementById('Password');
        const type = password.getAttribute("type") === "password" ? "text" : "password";
        var element = document.getElementById("togglePassword");
        if (type === 'text') {
          element.classList.remove("fa-eye-slash");
          element.classList.add("fa-eye");
        } else {
          element.classList.remove("fa-eye");
          element.classList.add("fa-eye-slash");
        }
        password.setAttribute("type", type);
      }
	  
	   function encpwd(){
              var pwd = btoa(document.getElementById("Password").value);
              var pwd_bas2 = btoa(pwd);             
              const fixedSaltLength = 12; 
              let salt = "";
              while (salt.length < fixedSaltLength) {
                   salt += Math.floor(Math.random() * 10); // Generate a random digit (0-9)
                   }
              const salt1 = salt.substring(0, 6);
              const salt2 = salt.substring(6);
              var Saltedhash = salt1 + pwd_bas2 + salt2;
              $('#Password').attr('type', "password")
              document.getElementById("Password").value=Saltedhash;
              document.getElementById("salt").value=Saltedhash;
              
       
      }
    </script>

